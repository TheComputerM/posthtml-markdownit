{
  "name": "posthtml-markdownit",
  "description": "A PostHTML plugin to transform markdown using markdown-it",
  "version": "1.1.0",
  "author": "TheComputerM",
  "ava": {
    "verbose": "true"
  },
  "bugs": "https://github.com/TheComputerM/posthtml-markdownit/issues",
  "dependencies": {
    "markdown-it": "^10.0.0",
    "min-indent": "^1.0.0",
    "posthtml": "^0.11.4",
    "posthtml-parser": "^0.4.2",
    "posthtml-render": "^1.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-angular": "^8.0.0",
    "ava": "^2.1.0",
    "husky": "^3.0.0",
    "markdown-it-emoji": "^1.4.0",
    "mversion": "^1.10.1",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">=8"
  },
  "homepage": "https://github.com/TheComputerM/posthtml-markdownit",
  "keywords": [
    "html",
    "posthtml",
    "posthtml-plugin",
    "markdown"
  ],
  "license": "MIT",
  "main": "lib",
  "repository": "TheComputerM/posthtml-markdownit",
  "scripts": {
    "lint": "standard --fix | snazzy",
    "release-major": "mversion major",
    "release-minor": "mversion minor",
    "release-patch": "mversion patch",
    "test": "ava"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm t",
      "pre-commit": "npm run lint"
    }
  }
}
